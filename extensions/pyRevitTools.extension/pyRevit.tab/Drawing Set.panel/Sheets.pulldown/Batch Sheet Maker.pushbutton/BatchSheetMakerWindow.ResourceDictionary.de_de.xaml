<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Title">Blatt-Ersteller (Stapelverarbeitung)</system:String>
    
    <system:String x:Key="InstructionsLine1">Geben Sie die Liste der Blattnummern und -namen in folgendem Format ein:</system:String>
    <system:String x:Key="SheetNumberLabel">Blattnummer</system:String>
    <system:String x:Key="TAB">TAB</system:String>
    <system:String x:Key="SheetNameLabel">Blattname</system:String>
    <system:String x:Key="SheetNameExample">BLATTNAME</system:String>
    <system:String x:Key="InstructionsLine2">Mehrere Blätter mit identischen Namen können mit Blattnummernbereichen aufgelistet werden:</system:String>
    <system:String x:Key="RangeOfSheetNumbers">Bereich von Blattnummern:</system:String>
    <system:String x:Key="RangeExample">z.B. A103--A106 oder A103::A106</system:String>
    <system:String x:Key="ListOfSheetNumbers">Liste von Blattnummern:</system:String>
    <system:String x:Key="ListExample">z.B. A104,A105 oder A104;A105</system:String>
    
    <system:String x:Key="CreateSheets">Blätter erstellen</system:String>
    <system:String x:Key="CreatePlaceholders">Platzhalter erstellen</system:String>
    <system:String x:Key="CreateButton">Erstellen</system:String>
</ResourceDictionary>
