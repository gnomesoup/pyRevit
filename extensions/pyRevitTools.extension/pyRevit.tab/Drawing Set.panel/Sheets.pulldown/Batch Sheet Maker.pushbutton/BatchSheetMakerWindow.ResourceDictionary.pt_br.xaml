<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Title">Criador de Folhas em Lote</system:String>
    
    <system:String x:Key="InstructionsLine1">Insira a lista de números e nomes de folhas neste formato:</system:String>
    <system:String x:Key="SheetNumberLabel">número-da-folha</system:String>
    <system:String x:Key="TAB">TAB</system:String>
    <system:String x:Key="SheetNameLabel">nome-da-folha</system:String>
    <system:String x:Key="SheetNameExample">NOME DA FOLHA</system:String>
    <system:String x:Key="InstructionsLine2">Múltiplas folhas com nomes idênticos podem ser listadas com intervalos de números de folha:</system:String>
    <system:String x:Key="RangeOfSheetNumbers">Intervalo de Números de Folha:</system:String>
    <system:String x:Key="RangeExample">ex: A103--A106 ou A103::A106</system:String>
    <system:String x:Key="ListOfSheetNumbers">Lista de Números de Folha:</system:String>
    <system:String x:Key="ListExample">ex: A104,A105 ou A104;A105</system:String>
    
    <system:String x:Key="CreateSheets">Criar Folhas</system:String>
    <system:String x:Key="CreatePlaceholders">Criar Espaços Reservados</system:String>
    <system:String x:Key="CreateButton">Criar</system:String>
</ResourceDictionary>