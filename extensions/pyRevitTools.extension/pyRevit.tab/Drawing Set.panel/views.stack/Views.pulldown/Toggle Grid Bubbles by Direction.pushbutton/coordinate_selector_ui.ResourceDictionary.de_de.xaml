<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Title">Rasterblasen-Einstellung</system:String>
    <system:String x:Key="HeaderTitle">Rasterblasen-Einstellung</system:String>
    <system:String x:Key="Subtitle">Koordinatensystem und Winkel-Toleranz konfigurieren</system:String>
    
    <system:String x:Key="CoordinateSystemHeader">Koordinatensystem</system:String>
    <system:String x:Key="AllGrids">Alle Raster</system:String>
    <system:String x:Key="AllGridsDescription">Schaltet alle Raster in der aktiven Ansicht um</system:String>
    <system:String x:Key="TrueNorth">Geografisch Nord</system:String>
    <system:String x:Key="TrueNorthDescription">Verwendet geografische Nordausrichtung</system:String>
    <system:String x:Key="ProjectNorth">Projekt Nord</system:String>
    <system:String x:Key="ProjectNorthDescription">Verwendet Projekt-Koordinatensystem (wendet den 'Winkel zu Geografisch Nord' an)</system:String>
    <system:String x:Key="ViewOrientation">Ansichts-/Sichtbereichsbox-Ausrichtung</system:String>
    <system:String x:Key="ViewOrientationDescription">Verwendet aktuelle Ansichtsausrichtung und Sichtbereichsboxen</system:String>
    
    <system:String x:Key="AngleToleranceHeader">Winkel-Toleranz</system:String>
    <system:String x:Key="StrictLabel">1° (Streng)</system:String>
    <system:String x:Key="PermissiveLabel">40° (Tolerant)</system:String>
    <system:String x:Key="HowThisWorks">Wie das funktioniert</system:String>
    <system:String x:Key="HowThisWorksDescription">Steuert, wie genau sich Raster mit dem gewählten Koordinatensystem ausrichten müssen. Niedrige Werte = nur gerade Raster. Hohe Werte = schließt schräge Raster ein.</system:String>
    
    <system:String x:Key="AngleDescription1">Sehr streng - nur perfekt ausgerichtete Raster</system:String>
    <system:String x:Key="AngleDescription5">Eng - gut für präzise Rasterlayouts</system:String>
    <system:String x:Key="AngleDescription10">Mäßig - gut für die meisten Fälle</system:String>
    <system:String x:Key="AngleDescription15">Entspannt - schließt leicht schräge Raster ein</system:String>
    <system:String x:Key="AngleDescription25">Tolerant - schließt mehr schräge Raster ein</system:String>
    <system:String x:Key="AngleDescription35">Sehr tolerant - schließt die meisten Ausrichtungen ein</system:String>
    <system:String x:Key="AngleDescription40">Maximale Toleranz - schließt stark schräge Raster ein</system:String>
    
    <system:String x:Key="Cancel">Abbrechen</system:String>
    <system:String x:Key="Continue">Fortfahren</system:String>
</ResourceDictionary>
