<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Window Title -->
    <system:String x:Key="ColorSplasher.Title">按参数值着色元素</system:String>

    <!-- Section Labels -->
    <system:String x:Key="ColorSplasher.Labels.CategorySelection">类别选择</system:String>
    <system:String x:Key="ColorSplasher.Labels.ParameterSelection">参数选择</system:String>
    <system:String x:Key="ColorSplasher.Labels.ValuesColorAssignment">值颜色分配</system:String>
    <system:String x:Key="ColorSplasher.Labels.ManageSchemes">管理方案</system:String>
    <system:String x:Key="ColorSplasher.Labels.GenerateColors">生成颜色</system:String>
    <system:String x:Key="ColorSplasher.Labels.ApplySettings">应用设置</system:String>

    <!-- Buttons -->
    <system:String x:Key="ColorSplasher.Buttons.SaveLoadScheme">保存 / 加载颜色方案</system:String>
    <system:String x:Key="ColorSplasher.Buttons.GradientColors">渐变颜色</system:String>
    <system:String x:Key="ColorSplasher.Buttons.RandomColors">随机颜色</system:String>
    <system:String x:Key="ColorSplasher.Buttons.SetColors">设置颜色</system:String>
    <system:String x:Key="ColorSplasher.Buttons.Reset">重置</system:String>
    <system:String x:Key="ColorSplasher.Buttons.CreateLegend">创建图例</system:String>
    <system:String x:Key="ColorSplasher.Buttons.CreateViewFilters">创建视图过滤器</system:String>
    <system:String x:Key="ColorSplasher.Buttons.SelectAll">全选</system:String>
    <system:String x:Key="ColorSplasher.Buttons.SelectNone">全不选</system:String>

    <!-- Checkboxes -->
    <system:String x:Key="ColorSplasher.Checkboxes.ApplyLineColor">应用线颜色</system:String>
    <system:String x:Key="ColorSplasher.Checkboxes.ApplyForegroundPattern">应用前景图案颜色</system:String>
    <system:String x:Key="ColorSplasher.Checkboxes.ApplyBackgroundPattern">应用背景图案颜色</system:String>
    <system:String x:Key="ColorSplasher.Checkboxes.ApplyBackgroundPattern.RequiresRevit2019">应用背景图案颜色（需要 Revit 2019 或更高版本）</system:String>

    <!-- Tooltips -->
    <system:String x:Key="ColorSplasher.Tooltips.CategorySelection">选择一个类别以开始着色。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.ParameterSelection">选择一个参数以根据其值对元素进行着色。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.ValuesColorAssignment">通过单击其值来重新分配颜色。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.SaveLoadScheme">保存当前颜色方案或加载现有方案。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.GradientColors">基于第一个和最后一个值的颜色，为所有值重新分配渐变颜色。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.RandomColors">为所有值重新分配新的随机颜色。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.SetColors">替换视图中每个元素的颜色。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.Reset">将 Revit 视图中的颜色重置为初始状态。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.CreateLegend">为所有值及其颜色创建新的图例视图。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.CreateViewFilters">为所有值及其颜色创建视图过滤器和规则。</system:String>
    <system:String x:Key="ColorSplasher.Tooltips.ApplyLineColor">启用后，将颜色应用于投影线颜色。</system:String>
