<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Title">转换线样式</system:String>
    <system:String x:Key="FromStyle">源样式</system:String>
    <system:String x:Key="ToStyle">目标样式</system:String>
    <system:String x:Key="ConvertSelectedStyles">转换选定的样式</system:String>
    <system:String x:Key="ConvertStyles">转换样式</system:String>
    <system:String x:Key="ConvertOptions">转换选项</system:String>
    <system:String x:Key="ConvertDetailLines">转换详图线</system:String>
