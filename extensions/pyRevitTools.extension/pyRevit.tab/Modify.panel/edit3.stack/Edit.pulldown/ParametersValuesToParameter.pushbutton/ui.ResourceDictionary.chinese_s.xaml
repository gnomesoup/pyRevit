<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Title">参数到参数 - 复制参数值</system:String>
    <system:String x:Key="SelectCategory">选择类别：</system:String>
    <system:String x:Key="SourceParameters">源参数（如需要可选择多个）：</system:String>
    <system:String x:Key="TargetParameter">目标参数（选择一个）：</system:String>
    <system:String x:Key="InsertSeparators">在由空格分隔的参数值之间插入分隔符。</system:String>
    <system:String x:Key="AddSpacesToSeparators">向分隔符添加空格：</system:String>
    <system:String x:Key="None">无</system:String>
    <system:String x:Key="Before">之前</system:String>
    <system:String x:Key="After">之后</system:String>
    <system:String x:Key="BeforeAndAfter">之前和之后</system:String>
    <system:String x:Key="Execute">执行</system:String>
    <system:String x:Key="Cancel">取消</system:String>
</ResourceDictionary>
