<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Default Title -->
    <system:String x:Key="SelectFromList.DefaultTitle">Выбор пользователя</system:String>

    <!-- Group Selector -->
    <system:String x:Key="SelectFromList.GroupSelector.Label">Группа списка:</system:String>
    
    <!-- Buttons -->
    <system:String x:Key="SelectFromList.CheckAll.Button">Выбрать все</system:String>
    <system:String x:Key="SelectFromList.UncheckAll.Button">Снять все</system:String>
    <system:String x:Key="SelectFromList.ToggleAll.Button">Инвертировать все</system:String>
    <system:String x:Key="SelectFromList.Reset.Button">Сбросить</system:String>
    <system:String x:Key="SelectFromList.Select.Button">Выбрать</system:String>
    
    <!-- Dynamic button labels (when filtered) -->
    <system:String x:Key="SelectFromList.Check.Button">Выбрать</system:String>
    <system:String x:Key="SelectFromList.Uncheck.Button">Снять</system:String>
    <system:String x:Key="SelectFromList.Toggle.Button">Инвертировать</system:String>
    
    <!-- Tooltips -->
    <system:String x:Key="SelectFromList.RegexToggle.Tooltip">Переключить между использованием регулярного выражения и нечеткой фильтрацией</system:String>
    
</ResourceDictionary>
