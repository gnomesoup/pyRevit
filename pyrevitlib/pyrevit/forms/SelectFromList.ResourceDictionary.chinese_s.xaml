<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Default Title -->
    <system:String x:Key="SelectFromList.DefaultTitle">用户输入</system:String>

    <!-- Group Selector -->
    <system:String x:Key="SelectFromList.GroupSelector.Label">列表组:</system:String>
    
    <!-- Buttons -->
    <system:String x:Key="SelectFromList.CheckAll.Button">全选</system:String>
    <system:String x:Key="SelectFromList.UncheckAll.Button">全部不选</system:String>
    <system:String x:Key="SelectFromList.ToggleAll.Button">切换全部</system:String>
    <system:String x:Key="SelectFromList.Reset.Button">重置</system:String>
    <system:String x:Key="SelectFromList.Select.Button">选择</system:String>
    
    <!-- Dynamic button labels (when filtered) -->
    <system:String x:Key="SelectFromList.Check.Button">勾选</system:String>
    <system:String x:Key="SelectFromList.Uncheck.Button">取消勾选</system:String>
    <system:String x:Key="SelectFromList.Toggle.Button">切换</system:String>
    
    <!-- Tooltips -->
    <system:String x:Key="SelectFromList.RegexToggle.Tooltip">在正则表达式和模糊筛选之间切换</system:String>
    
</ResourceDictionary>
